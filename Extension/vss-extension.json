{
    "manifestVersion": 1,
    "id": "gl-vsts-tasks-yarn",
    "name": "Yarn",
    "version": "0.0.0",
    "publisher": "geeklearningio",    
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Harness Yarn power and make your build faster and more robust",
