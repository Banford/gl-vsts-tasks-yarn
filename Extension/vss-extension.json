            "uri": "https://github.com/geeklearningio/gl-vsts-tasks-yarn/wiki"
        },
        "support": {
            "uri": "https://github.com/geeklearningio/gl-vsts-tasks-yarn/issues"
        },
        "repository": {
            "uri": "https://github.com/geeklearningio/gl-vsts-tasks-yarn"
        },
        "issues": {
            "uri": "https://github.com/geeklearningio/gl-vsts-tasks-yarn/issues"
        }
    },
    "branding": {
        "color": "#008B8B",
        "theme": "dark"
    },  
    "files": [
        {
            "path": "Tasks"
        },
        {
            "path": "Screenshots",
            "addressable": true
        }
    ]
